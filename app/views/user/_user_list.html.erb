
  <% for user in @users %>
<tr>
  <td class='first'>
  <%= link_to user.username, edit_user_path(user.id) %>
  </td>
  <td><%= user.email %>
  </td>
  <td><%= user.status %>
  </td>
          <td class='last'><center><%= link_to "X", {:action => 'destroy', :uid => user.id} -%></center>
  </td>
</tr>
<% end %>


